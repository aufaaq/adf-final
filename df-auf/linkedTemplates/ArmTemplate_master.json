{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-auf"
		},
		"Snowflake1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"AzureBlobStorage1_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorage2'"
		},
		"Snowflake1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mini"
		},
		"Snowflake1_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "accountadmin"
		},
		"Snowflake1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "RDOSWTF-FRB47736.snowflakecomputing.com"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "JM-DKT-018"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Event_Booking"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "AzureAD\\AufaaqAhamed"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-auf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Snowflake1_password": {
						"value": "[parameters('Snowflake1_password')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"AzureBlobStorage1_sasUri": {
						"value": "[parameters('AzureBlobStorage1_sasUri')]"
					},
					"AzureBlobStorage2_sasUri": {
						"value": "[parameters('AzureBlobStorage2_sasUri')]"
					},
					"Snowflake1_properties_typeProperties_database": {
						"value": "[parameters('Snowflake1_properties_typeProperties_database')]"
					},
					"Snowflake1_properties_typeProperties_role": {
						"value": "[parameters('Snowflake1_properties_typeProperties_role')]"
					},
					"Snowflake1_properties_typeProperties_host": {
						"value": "[parameters('Snowflake1_properties_typeProperties_host')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}